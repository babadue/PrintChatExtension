{
    "manifest_version": 3,
    "name": "Print ChatGPT Conversation",
    "version": "1.0",
    "description": "Adds a button to print the entire ChatGPT conversation.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://chat.openai.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ]
  }